<div class="container vh-100 d-flex justify-content-center">
  <div class="row align-items-center">
    <h1>New list</h1>
      <%= form_with model: @list, class:'form' do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %>
        <%= f.label :movie_category %>
        <%= f.text_field :genre %>
        <%= f.submit "create"%>
      <% end %>
    <p>
      <a href="<%= lists_path %>" class="btn btn-primary m-4" style="text-decoration: none;">go back
      </a>
    </p>
  </div>
</div>
